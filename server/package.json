{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.7",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.26.0",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "supertest": "^6.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js*": [
      "npm run lint:fix",
      "npm run test:staged",
      "git add"
    ]
  },
  "scripts": {
    "start": "env-cmd -f ./config/dev.env nodemon server",
    "build": "react-scripts build",
    "test": "env-cmd -f ./config/test.env jest --config ./jest.config.js --detectOpenHandles",
    "test:coverage": "env-cmd -f ./config/test.env jest --config ./jest.config.js --detectOpenHandles --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint **/*.js*",
    "lint:fix": "prettier-eslint **/*.js* --write",
    "test:staged": "env-cmd -f ./config/test.env jest --config ./jest.config.js --findRelatedTests *"
  }
}
