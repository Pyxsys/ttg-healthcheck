    "@types/react-burger-menu": "^2.8.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "chromedriver": "^95.0.0",
    "cypress": "^9.0.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-react": "^7.26.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts*": [
      "npm run lint:fix",
      "npm run test:staged",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test src --watchAll=false",
    "test:coverage": "react-scripts test src --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint **/*.ts*",
    "lint:fix": "prettier-eslint **/*.ts* --write",
    "test:staged": "react-scripts test src --watchAll=false --findRelatedTests"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000/",
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
